# Definition of client-server interface for the UTT Demo test application.

Msg PublicTx 3 {
    string tx
}

Msg UttTx 4 {
    string tx
}

Msg TxReply 5 {
    bool success
    uint64 last_block_id
    string err
}

Msg GetLastBlockRequest 6 {
}

Msg GetLastBlockReply 7 {
    uint64 last_block_id
}

Msg GetBlockDataRequest 8 {
    uint64 block_id
}

Msg GetBlockDataReply 9 {
    uint64 block_id
    oneof {
        PublicTx
        UttTx
    } tx
}

Msg UTTRequest 1 {
    oneof {
        PublicTx
        UttTx
        GetLastBlockRequest
        GetBlockDataRequest
    } request
}

Msg UTTReply 2 {
    oneof {
        TxReply
        GetLastBlockReply
        GetBlockDataReply
    } reply
}
